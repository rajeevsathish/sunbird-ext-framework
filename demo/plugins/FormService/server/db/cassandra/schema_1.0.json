
{
  "type": "cassandra",
  "config": {
  },
  "keyspace_name": "form_service",
  "column_families": [
    {
      "table_name": "form_data",
      "fields": {
        "type": {
          "type": "varchar"
        },
        "subtype": {
          "type": "varchar",
          "default": "*"
        },
        "action": {
          "type": "varchar"
        },
        "component": {
          "type": "varchar",
          "default": "*"
        },
        "root_org": {
          "type": "varchar",
          "default": "*"
        },
        "framework": {
          "type": "varchar",
          "default": "*"
        },
        "data": {
          "type": "varchar"
        },
        "created_on": {
          "type": "timestamp",
          "default": {"$db_function": "toTimestamp(now())"}
        },
        "last_modified_on": {
          "type": "timestamp"
        },
        "isretired": {
          "type": "varchar"
        },
        "version": {
          "type": "varchar",
          "default": "v1"
        },
        "vesionhistory":{
          "type": "varchar"
        }

      },
      "key": [ ["root_org", "framework", "type", "subtype", "action", "component", "version", "isretired"] ]
    }
  ]
}